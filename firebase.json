{
  "hosting": {
    "public": "web_admin",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [
      {
        "source": "**/*.@(js|css)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "max-age=3600"
          }
        ]
      }
    ],
    "redirects": [
      {
        "source": "/",
        "destination": "/admin_dashboard.html",
        "type": 301
      }
    ]
  }
}
